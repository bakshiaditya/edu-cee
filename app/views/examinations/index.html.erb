<h1>Listing examinations</h1>

<table>
  <tr>
    <th>Institution</th>
    <th>Subject</th>
    <th>Chapter</th>
    <th>Total marks</th>
    <th>Total time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @examinations.each do |examination| %>
  <tr>
    <td><%= examination.institution_id %></td>
    <td><%= examination.subject %></td>
    <td><%= examination.chapter %></td>
    <td><%= examination.total_marks %></td>
    <td><%= examination.total_time %></td>
    <td><%= link_to 'Show', examination %></td>
    <td><%= link_to 'Edit', edit_examination_path(examination) %></td>
    <td><%= link_to 'Destroy', examination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Examination', new_examination_path %>
